<UserControl x:Class="SignalCompressionMUI.Views.ZedGraphSpectrumView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:mui="http://firstfloorsoftware.com/ModernUI"
             xmlns:zedGraph="clr-namespace:ZedGraph;assembly=ZedGraph"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Style="{StaticResource ContentRoot}" Margin="0 10 10 0">
        <WindowsFormsHost Name="windowsFormsHost1" Margin="0">

            <zedGraph:ZedGraphControl x:Name="zedGraph" BackColor="Transparent"
                             Width="1200"
                             Height="600" />
        </WindowsFormsHost>
    </Grid>
</UserControl>
